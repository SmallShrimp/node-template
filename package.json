{
  "name": "nt",
  "version": "1.0.0",
  "description": "node template",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.js",
    "buildDev": "cross-env NODE_ENV=development webpack --config ./webpack/dev/webpack.browser.config.js && webpack --config ./webpack/dev/webpack.node.config.js",
    "start": "cross-env NODE_ENV=production node build/app",
    "startDev": "cross-env NODE_ENV=development node build/app",
    "dev": "parallelshell \"npm run webpackServer\" \"npm run watchServer\"",
    "watchServer": "nodemon -e js,jsx,json -w index.js -w src -i src/client -x \"webpack --config ./webpack/dev/webpack.node.config.js && npm run startDev\"",
    "webpackServer": "webpack-dev-server --config ./webpack/dev/webpack.browser.config.js --inline --hot --quiet --display-error-details",
    "lint": "eslint lib/** --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "md5": "^2.2.1",
    "memory-cache": "^0.1.6",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bower-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "source-map-support": "^0.4.6",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  }
}
