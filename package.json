{
  "name": "nt",
  "version": "1.0.0",
  "description": "node template",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.node.config.js",
    "start": "cross-env NODE_ENV=production node build/app",
    "dev": "parallelshell \"npm run watchServer\" ",
    "startDev": "cross-env NODE_ENV=development node build/app",
    "watchServer": "nodemon -e js,jsx,json -w index.js -w src -i lib/client -x \"webpack --config ./webpack/webpack.node.config.js && npm run startDev\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bower-webpack-plugin": "^0.1.9",
    "cross-env": "^3.1.3",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0",
    "source-map-support": "^0.4.6",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  }
}
