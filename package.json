{
  "name": "nt",
  "version": "1.0.0",
  "description": "node template",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.js",
    "buildDev": "cross-env NODE_ENV=development webpack --config ./webpack/dev/webpack.browser.config.js && webpack --config ./webpack/dev/webpack.node.config.js",
    "start": "cross-env NODE_ENV=production node build/app",
    "startDev": "cross-env NODE_ENV=development node build/app",
    "dev": "parallelshell \"npm run webpackServer\" \"npm run watchServer\"",
    "watchServer": "nodemon -e js,jsx,json -w index.js -w src -i src/client -x \"webpack --config ./webpack/dev/webpack.node.config.js && npm run startDev\"",
    "webpackServer": "webpack-dev-server --config ./webpack/dev/webpack.browser.config.js --inline --hot --quiet --display-error-details",
    "lint": "eslint lib/** --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "lodash": "^4.17.2",
    "md5": "^2.2.1",
    "memory-cache": "^0.1.6",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0"
  },
